{
  "name": "guestware",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/auspicus/guestware.git"
  },
  "main": "./lib/index.js",
  "license": "MIT",
  "scripts": {
    "test": "npm run build && jest",
    "docs": "typedoc --module commonjs --out docs src",
    "postinstall": "npm run build",
    "build": "tsc --declaration --outDir ./lib",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-success": "echo 'Pre-commit checks passed! Committing changes.' && exit 0"
  },
  "precommit": [
    "precommit-msg",
    "test",
    "precommit-success"
  ],
  "dependencies": {
    "node-fetch": "^2.2.0",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/node": "^10.12.1",
    "@types/node-fetch": "^2.1.2",
    "@types/xmldom": "^0.1.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "pre-commit": "^1.2.2",
    "regenerator-runtime": "^0.13.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.1.4"
  }
}
